<!-- checks to make sure at least one defense type and no more than two defenses are selected -->
<script type="text/javascript">
    function chkcontrol(j) {var total=0; 
        for(var i=0; i < document.def.type.length; i++)
        {if(document.def.type[i].checked) 
        {total =total +1;}
    
        else if(total > 2)
        {alert("Please select only two types for Defense")
        document.def.type[j].checked= false ;
        return false;}}
        }
    </script>